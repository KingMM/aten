<MakeItSo>
  <AllProjects>
    <CPPFolderPrefix prefix=""/>
    <RemoveLibraryPath path="../3rdparty/glfw/x64/src/Debug"/>
    <RemoveLibraryPath path="../3rdparty/glfw/x64/src/Release"/>
    <RemoveLibraryPath path="../3rdparty/glew/lib/Debug/x64"/>
    <RemoveLibraryPath path="../3rdparty/glew/lib/Release/x64"/>
    <RemoveConfiguration config="Debug|Win32"/>
    <RemoveConfiguration config="Profile|Win32"/>
    <RemoveConfiguration config="Profile|x64"/>
    <RemoveConfiguration config="Release|Win32"/>
    <RemoveLibrary library="glew32d"/>
    <RemoveLibrary library="glew32"/>
    <RemoveLibrary library="glfw3"/>
    <RemoveLibrary library="OpenGL32"/>
    <RemoveLibrary library="Shlwapi"/>
    <RemoveFilesAndDirectories regex="windows"/>
    <AddCompilerFlag configuration="Debug" flag="-std=c++11 -fopenmp"/>
    <AddCompilerFlag configuration="Release" flag="-std=c++11 -fopenmp"/>
    <ReplaceProjectRootFolderRelative folder="./"/>
  </AllProjects>
  <Project name="appaten">
    <AddLibrary configuration="Debug" library="GLEW"/>
    <AddLibrary configuration="Debug" library="glfw"/>
    <AddLibrary configuration="Debug" library="GL"/>
    <AddLibrary configuration="Debug" library="-fopenmp"/>
    <AddLibrary configuration="Release" library="GLEW"/>
    <AddLibrary configuration="Release" library="glfw"/>
    <AddLibrary configuration="Release" library="GL"/>
    <AddLibrary configuration="Release" library="-fopenmp"/>
  </Project>
</MakeItSo>
